{
  "name": "@pod-protocol/sdk-js",
  "version": "1.5.0",
  "description": "JavaScript SDK for PoD Protocol (Prompt or Die) AI Agent Communication Protocol",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build:prod && npm run test",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "keywords": [
    "solana",
    "ai-agents",
    "agent-communication",
    "blockchain-protocol",
    "web3-sdk",
    "decentralized-messaging",
    "prompt-or-die",
    "pod-protocol",
    "javascript",
    "anchor"
  ],
  "author": "PoD Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dexploarer/PoD-Protocol.git",
    "directory": "sdk-js"
  },
  "bugs": {
    "url": "https://github.com/Dexploarer/PoD-Protocol/issues"
  },
  "homepage": "https://github.com/Dexploarer/PoD-Protocol#readme",
  "dependencies": {
    "@solana/web3.js": "1.98.2",
    "@coral-xyz/anchor": "0.31.1",
    "node-fetch": "3.3.2",
    "crypto-js": "4.2.0",
    "axios": "1.10.0"
  },
  "devDependencies": {
    "@types/node": "24.0.3",
    "@typescript-eslint/eslint-plugin": "8.35.0",
    "@typescript-eslint/parser": "8.35.0",
    "eslint": "9.29.0",
    "jest": "30.0.2",
    "rollup": "4.44.0",
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-commonjs": "28.0.6",
    "@rollup/plugin-typescript": "12.1.3",
    "typescript": "5.8.3",
    "jsdoc": "4.0.4"
  },
  "peerDependencies": {
    "@solana/web3.js": "1.98.2"
  }
}

{
  "name": "@pod-protocol/sdk-js",
  "version": "1.5.0",
  "description": "JavaScript SDK for PoD Protocol (Prompt or Die) AI Agent Communication Protocol",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:prod": "rollup -c --environment NODE_ENV:production",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.ts",
    "lint:fix": "eslint src --ext .js,.ts --fix",
    "clean": "rm -rf dist/",
    "prepublishOnly": "npm run clean && npm run build:prod && npm run test",
    "docs": "jsdoc -c jsdoc.conf.json"
  },
  "keywords": [
    "solana",
    "ai-agents",
    "agent-communication",
    "blockchain-protocol",
    "web3-sdk",
    "decentralized-messaging",
    "prompt-or-die",
    "pod-protocol",
    "javascript",
    "anchor"
  ],
  "author": "PoD Protocol Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Dexploarer/PoD-Protocol.git",
    "directory": "sdk-js"
  },
  "bugs": {
    "url": "https://github.com/Dexploarer/PoD-Protocol/issues"
  },
  "homepage": "https://github.com/Dexploarer/PoD-Protocol#readme",
  "dependencies": {
    "@solana/web3.js": "^1.95.3",
    "@coral-xyz/anchor": "^0.30.1",
    "node-fetch": "^3.3.2",
    "crypto-js": "^4.2.0",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "rollup": "^4.22.4",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "typescript": "^5.6.2",
    "jsdoc": "^4.0.3"
  },
  "peerDependencies": {
    "@solana/web3.js": "^1.95.3"
  }
}

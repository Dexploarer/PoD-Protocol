{
  "name": "@pod-protocol/sdk",
  "version": "1.5.2",
  "description": "Pod Protocol SDK for TypeScript",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c && tsc --noEmit false --skipLibCheck",
    "build:ignore-errors": "tsc --noEmit false --skipLibCheck --noEmitOnError false",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@solana-program/compute-budget": "^0.8.0",
    "@solana-program/system": "^0.7.0",
    "@solana-program/token": "^0.5.1",
    "@solana/web3.js": "^2.0.0",
    "multiformats": "^13.3.7"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.1",
    "@codama/renderers": "^1.0.24",
    "@codama/visitors-core": "^1.3.0",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/pluginutils": "^5.2.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "rollup": "^4.44.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "solana",
    "blockchain",
    "web3",
    "pod-protocol"
  ],
  "author": "Pod Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pod-protocol/pod-protocol.git",
    "directory": "sdk"
  }
}

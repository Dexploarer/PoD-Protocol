{
  "name": "@pod-protocol/sdk",
  "version": "1.5.2",
  "description": "Pod Protocol SDK for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc --noEmit false --skipLibCheck",
    "build:ignore-errors": "tsc --noEmit false --skipLibCheck --noEmitOnError false",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@solana/web3.js": "^2.0.0",
    "@solana-program/system": "^0.7.0",
    "@solana-program/token": "^0.5.1",
    "@solana-program/compute-budget": "^0.8.0"
  },
  "devDependencies": {
    "@codama/nodes-from-anchor": "^1.2.1",
    "@codama/renderers": "^1.0.24",
    "@codama/visitors-core": "^1.3.0",
    "@types/node": "^18.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "solana",
    "blockchain",
    "web3",
    "pod-protocol"
  ],
  "author": "Pod Protocol",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pod-protocol/pod-protocol.git",
    "directory": "sdk"
  }
}
